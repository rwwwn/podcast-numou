# Resend API for sending emails
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=

# Contact form email configuration
# From address (must be verified in Resend)
CONTACT_FROM=

# Inbox email where contact form submissions will be sent
CONTACT_INBOX_TO=

# Upstash Redis for rate limiting
# Get your credentials from: https://console.upstash.com/
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# hCaptcha for bot protection
# Get your keys from: https://www.hcaptcha.com/
HCAPTCHA_SITE_KEY=
HCAPTCHA_SECRET=

# Supabase database
# Get your credentials from: https://app.supabase.com/
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
